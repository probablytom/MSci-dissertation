@article{Burns2006,
abstract = {In this chapter we have argued that complex systems exhibit behaviour at many different time levels and that a useful aid in structuring, describing and specifying such behaviour is to use time bands. Viewing a system as a collection of activities within a finite set of bands is an effective means of separating concerns and identifying inconsistencies between different â€˜layers' of the system. Time bands are not mapped on to a single notion of physical time. Within a system there will always be a relation between bands but the bands need not be tightly synchronised. There is always some level of imprecision between any two adjacent bands. Indeed the imprecision may be large in social systems and be a source of dependability (robustness).},
author = {Burns, Alan and Baxter, G},
doi = {10.1007/1-84628-111-3_4},
file = {:Users/tom/Downloads/Time{\_}bands{\_}in{\_}systems{\_}structure.pdf:pdf},
isbn = {1-84628-110-5},
journal = {Structure for Dependability: Computer-Based Systems from an Interdiciplinary Perspective},
keywords = {literature review IDE,time bands},
pages = {74--88},
title = {{Time Bands in Systems Structure}},
year = {2006}
}

